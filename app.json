{
  "expo": {
    "name": "INSIGHT",
    "slug": "INSIGHT",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
